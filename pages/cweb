<script type="text/javascript">
	const li_cards_pages=document.getElementsByClassName('cardpageimg');

	// document.getElementById('exshadow').addEventListener('mouseenter',function(){
	// 	document.getElementById('exshadow').classList.remove('shadow');
	// 	document.getElementById('exshadow').classList.add('shadow-lg');
	// 	document.getElementById('exshadow').style.transition = "all 1s";
	// });
	// document.getElementById('exshadow').addEventListener('mouseleave',function(){
	// 	document.getElementById('exshadow').classList.remove('shadow-lg');
	// 	document.getElementById('exshadow').classList.add('shadow');
	// });
	document.querySelectorAll('#list_pagesp cardpageimg').forEach(function(cardpage, index){
		cardpage.addEventListener('mouseenter', event=>{
			cardpage.classList.remove('shadow');
			cardpage.classList.add('shadow-lg');
			// li_cards_pages.style.transition = "all 2s";
		});
		cardpage.addEventListener('mouseleave', event=>{
			cardpage.classList.classList.remove('shadow-lg');
			cardpage.classList.add('shadow');

		});
	});
</script>

